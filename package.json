{
  "name": "auth0-extension-hapi-tools",
  "version": "0.1.0",
  "description": "A set of tools and utilities to simplify the development of Auth0 Extensions with Hapi.",
  "main": "src/index.js",
  "dependencies": {
    "auth0-extension-tools": "^0.14.0",
    "boom": "^4.1.0",
    "express-jwt": "^5.0.0",
    "jwks-rsa": "^1.1.1",
    "webtask-tools": "^2.2.0"
  },
  "devDependencies": {
    "autochecker": "^0.9.2",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "faucet": "0.0.1",
    "nock": "^8.0.0",
    "nyc": "^8.1.0",
    "tape": "^4.6.0"
  },
  "scripts": {
    "release": "git tag $npm_package_version && git push && git push --tags && npm publish",
    "test": "./node_modules/tape/bin/tape tests/**/*.js | ./node_modules/faucet/bin/cmd.js",
    "test:docker": "./node_modules/autochecker/cli.js 4.4.5 6.3.1",
    "test:coverage": "./node_modules/nyc/bin/nyc.js ./node_modules/tape/bin/tape tests/**/*.js | ./node_modules/faucet/bin/cmd.js && ./node_modules/nyc/bin/nyc.js report",
    "prepublish": "npm run test:docker"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0-extensions/auth0-extension-express-tools.git"
  },
  "keywords": [
    "auth0",
    "extensions",
    "hapi",
    "webtask"
  ],
  "author": "Auth0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0-extensions/auth0-extension-express-tools/issues"
  },
  "homepage": "https://github.com/auth0-extensions/auth0-extension-express-tools#readme"
}
